{% assign items = "" | split: "," %}{% assign my_size = page.path | split: '/' | size | plus:1 %}{% assign my_path = page.path | split: '/' | pop | join: '/' | append: '/' | append: page.slug | append: '/' %}{% assign feeds = site.feeds | sort: 'weight' | where_exp:'item', 'item.path contains my_path' %}{% for feed in feeds %}{% assign feed_size = feed.path | split: '/' | size %}{% if feed_size == my_size %}{% assign items = items | push:feed %}{% endif %}{% endfor %}{% assign data = null %}{% assign my_data = site.data %}{% assign my_hash = page.path | split: '/' | pop %}{% for hash in my_hash %}{% if hash contains '_' %}{% assign my_data = my_data[hash] %}{% else %}{% assign _hash = hash | prepend: '_' %}{% assign my_data = my_data[_hash] %}{% endif %}{% endfor %}{% for hash in my_data %}{% if hash[1].title == page.slug %}{% assign data = hash[1] %}{% endif %}{% endfor %}{% if page.category %}{% include {{ page.path | remove_first: "_" | replace: ".md", ".html" }} %}{% endif %}